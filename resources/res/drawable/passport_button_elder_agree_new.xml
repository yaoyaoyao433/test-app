<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="10dp"/>
            <gradient android:startColor="@color/passport_elder_agree_button_disable_bg_start_color" android:endColor="@color/passport_elder_agree_button_disable_bg_end_color" android:angle="0"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp"/>
            <gradient android:startColor="@color/passport_elder_agree_button_press_bg_start_color" android:endColor="@color/passport_elder_agree_button_press_bg_end_color" android:angle="0"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="10dp"/>
            <gradient android:startColor="@color/passport_elder_agree_button_enable_bg_start_color" android:endColor="@color/passport_elder_agree_button_enable_bg_end_color" android:angle="0"/>
        </shape>
    </item>
</selector>
