<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/cb_background">
        <shape android:shape="oval">
            <solid android:color="@color/roo_cb_bg_solid_color_default"/>
            <size android:height="@dimen/roo_cb_bg_height_default" android:width="@dimen/roo_cb_bg_width_default"/>
        </shape>
    </item>
    <item android:gravity="center" android:id="@+id/cb_icon" android:bottom="-1dp">
        <inset android:drawable="@drawable/roo_check_box_icon_svg"/>
    </item>
</layer-list>
