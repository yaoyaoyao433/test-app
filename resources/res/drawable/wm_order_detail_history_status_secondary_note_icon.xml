<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center" android:height="6dp" android:width="6dp">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="0dp" android:thickness="3dp">
            <solid android:color="#ffd161"/>
        </shape>
    </item>
    <item android:gravity="center" android:height="8dp" android:width="8dp">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="3dp" android:thickness="1dp">
            <solid android:color="@android:color/white"/>
        </shape>
    </item>
    <item android:height="20dp" android:width="20dp">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="4dp" android:thickness="6dp">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>
