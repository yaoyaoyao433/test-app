<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#00ffffff" android:endColor="#00ffffff" android:angle="0" android:centerY="0.75"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="#00ffffff" android:endColor="#00ffffff" android:angle="0"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/videolib_main_color" android:endColor="@color/videolib_main_color" android:angle="0"/>
            </shape>
        </clip>
    </item>
</layer-list>
