<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <solid android:background="#88000000"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/wm_common_btn_bg" android:endColor="@color/wm_common_btn_bg" android:angle="270" android:centerY="0.75" android:centerColor="@color/wm_common_btn_bg"/>
                <stroke android:width="1dp" android:color="@color/wm_common_btn_bg"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="@color/wm_common_btn_bg" android:endColor="@color/wm_common_btn_bg" android:angle="270" android:centerY="0.75" android:centerColor="@color/wm_common_btn_bg"/>
            </shape>
        </clip>
    </item>
</layer-list>
