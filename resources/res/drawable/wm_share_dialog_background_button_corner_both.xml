<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/wm_common_dialog_button_pressed"/>
            <corners android:bottomLeftRadius="@dimen/wm_common_dialog_corner_radius" android:bottomRightRadius="@dimen/wm_common_dialog_corner_radius"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/wm_common_dialog_background"/>
            <corners android:bottomLeftRadius="@dimen/wm_common_dialog_corner_radius" android:bottomRightRadius="@dimen/wm_common_dialog_corner_radius"/>
        </shape>
    </item>
</selector>
