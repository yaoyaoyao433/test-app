<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" android:background="@color/transparent" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:layout_gravity="center_horizontal" android:orientation="vertical" android:background="@drawable/ptim_commonbus_video_double_check_bg" android:paddingTop="20dp" android:paddingBottom="12dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:textSize="14sp" android:textColor="#666666" android:layout_gravity="center_horizontal" android:paddingLeft="24dp" android:paddingRight="24dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="返回后将不保存已拍摄视频，确认返回?"/>
        <LinearLayout android:paddingLeft="12dp" android:paddingRight="12dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="24dp">
            <TextView android:textSize="14sp" android:textColor="#222222" android:gravity="center" android:id="@+id/video_operate_cancel" android:background="@drawable/ptim_commonbus_video_double_check_cancel_bg" android:paddingTop="10dp" android:paddingBottom="10dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="4dp" android:text="取消" android:layout_weight="1"/>
            <TextView android:textSize="14sp" android:textColor="#222222" android:gravity="center" android:id="@+id/video_operate_back" android:background="@drawable/ptim_commonbus_video_double_check_back_bg" android:paddingTop="10dp" android:paddingBottom="10dp" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="4dp" android:text="返回" android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
