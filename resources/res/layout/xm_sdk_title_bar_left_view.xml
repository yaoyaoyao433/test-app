<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/root_view_xm_sdk_title_bar_left" android:layout_alignParentLeft="true" android:layout_centerVertical="true" style="@style/xm_sdk_title_bar_sub_container">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/ly_back_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:contentDescription="@string/xm_sdk_btn_back">
        <ImageView android:id="@+id/img_back" android:layout_width="24dp" android:layout_height="24dp" android:contentDescription="@string/xm_sdk_btn_back" app:srcCompat="@drawable/xm_sdk_selector_ic_back"/>
        <TextView android:id="@+id/text_back" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="3dp" android:text="@string/xm_sdk_btn_back" android:includeFontPadding="false"/>
    </LinearLayout>
    <TextView android:id="@+id/text_close" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/xm_sdk_title_bar_text_close" android:includeFontPadding="false"/>
</LinearLayout>
