<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="horizontal" android:id="@+id/label_container" android:padding="0.5dp" android:layout_width="wrap_content" android:layout_height="wrap_content">
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/label_view_main1" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent">
        <LinearLayout android:gravity="center_vertical" android:layout_gravity="center_vertical" android:orientation="horizontal" android:id="@+id/label_view_main" android:layout_width="wrap_content" android:layout_height="match_parent">
            <ImageView android:id="@+id/main_icon" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="1dp"/>
            <TextView android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/main_title" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true"/>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout android:layout_gravity="center" android:id="@+id/deliver_line_layout" android:visibility="gone" android:layout_width="0.5dp" android:layout_height="match_parent">
        <View android:layout_gravity="center" android:id="@+id/deliver_line" android:layout_width="0.5dp" android:layout_height="8dp"/>
    </FrameLayout>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/label_view_second1" android:layout_width="wrap_content" android:layout_height="match_parent">
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/label_view_second" android:layout_width="wrap_content" android:layout_height="match_parent">
            <ImageView android:id="@+id/second_icon" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="1dp"/>
            <TextView android:ellipsize="end" android:gravity="center_vertical" android:id="@+id/second_title" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
