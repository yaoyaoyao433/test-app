<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:background="@android:color/white" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:id="@+id/eunomia_request_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
            <TextView android:textSize="18sp" android:textColor="@android:color/black" android:id="@+id/eunomia_request_header" android:background="#f5f5f5" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="请求"/>
            <TextView android:textSize="14sp" android:textColor="#a9a9a9" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="(请求采用MAPI标准缓存5min，这里不会主动清除缓存，如需清除，请点开Debug浮标手动删除MAPI缓存)"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/eunomia_clear_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
            <TextView android:textSize="18sp" android:textColor="@android:color/black" android:id="@+id/eunomia_clear_header" android:background="#f5f5f5" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="清除"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/eunomia_template_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
            <TextView android:textSize="18sp" android:textColor="@android:color/black" android:id="@+id/eunomia_template_header" android:background="#f5f5f5" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="模板"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/eunomia_module_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
            <TextView android:textSize="18sp" android:textColor="@android:color/black" android:id="@+id/eunomia_module_header" android:background="#f5f5f5" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="模块"/>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/eunomia_other_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
            <TextView android:textSize="18sp" android:textColor="@android:color/black" android:id="@+id/eunomia_other_header" android:background="#f5f5f5" android:padding="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="其他"/>
        </LinearLayout>
    </LinearLayout>
</ScrollView>
