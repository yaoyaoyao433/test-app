                <TextView android:textSize="10sp" android:textColor="@color/wm_shopcart_text_hint" android:ellipsize="end" android:id="@+id/txt_self_deliver" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:maxLines="1" android:layout_weight="1"/>
                <TextView android:textSize="11sp" android:textColor="@color/takeout_white" android:ellipsize="end" android:id="@+id/txt_cross_tip" android:focusable="false" android:focusableInTouchMode="false" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:lines="1" android:layout_weight="1" android:importantForAccessibility="no"/>
