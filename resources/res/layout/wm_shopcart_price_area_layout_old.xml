            <ImageView android:id="@+id/iv_city_delivery" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="5dp" android:src="@drawable/wm_shopcart_city_delivery_icon"/>
        </LinearLayout>
    </LinearLayout>
    <FrameLayout android:id="@+id/ll_empty_shopping_cart" android:paddingLeft="10dp" android:paddingRight="5dp" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content">
                <TextView android:textSize="14sp" android:textColor="@color/takeout_white" android:id="@+id/empty_shopping_cart_price_hint" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="¥" android:importantForAccessibility="no"/>
                <TextView android:textSize="22sp" android:textColor="@color/takeout_white" android:id="@+id/empty_shopping_cart_price" android:paddingLeft="1dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0"/>
                <TextView android:textSize="11sp" android:textColor="#b2b2b2" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/txt_dealInfo_multi_tip_empty_cart" android:focusable="false" android:focusableInTouchMode="false" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="1dp" android:text="最终价格以提单为准" android:maxLines="1" android:importantForAccessibility="no" android:layout_marginStart="4dp"/>
                <View android:id="@+id/divider_price_tip" android:background="@color/wm_shopcart_gray_line" android:layout_width="0.5dp" android:layout_height="14dp" android:layout_marginLeft="6dp" android:layout_marginTop="9dp"/>
                <TextView android:textSize="11sp" android:textColor="@color/wm_shopcart_gray_hint" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/tv_price_tip" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="6dp" android:layout_marginTop="1dp" android:singleLine="true" android:includeFontPadding="false" android:drawablePadding="5dp"/>
            </LinearLayout>
            <TextView android:textSize="11sp" android:textColor="@color/wm_shopcart_gray_hint" android:ellipsize="end" android:layout_gravity="center_vertical" android:id="@+id/tv_price_tip_new" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:singleLine="true" android:includeFontPadding="false" android:drawablePadding="5dp"/>
        </LinearLayout>
    </FrameLayout>
</LinearLayout>
