<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/shopcart_price_container_content" android:layout_width="match_parent" android:layout_height="match_parent" android:baselineAligned="false">
    <LinearLayout android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/layout_price" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
        <android.support.constraint.ConstraintLayout android:id="@+id/layout_price_value" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="11sp" android:textColor="@color/takeout_white" android:id="@+id/txt_dealInfo_price_prefix" android:focusable="false" android:focusableInTouchMode="false" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:importantForAccessibility="no" app:layout_constraintBaseline_toBaselineOf="@+id/txt_dealInfo_price_sign" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toStartOf="0"/>
            <TextView android:textSize="14sp" android:textColor="@color/takeout_white" android:id="@+id/txt_dealInfo_price_sign" android:focusable="false" android:focusableInTouchMode="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="¥" android:importantForAccessibility="no" android:layout_marginStart="4dp" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/txt_dealInfo_price_prefix" app:layout_goneMarginStart="0dp"/>
            <TextView android:textSize="21sp" android:textColor="@color/takeout_white" android:id="@+id/txt_dealInfo_price" android:focusable="false" android:focusableInTouchMode="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="1dp" android:text="0" android:maxLines="1" android:importantForAccessibility="no" app:layout_constraintBaseline_toBaselineOf="@+id/txt_dealInfo_price_sign" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/txt_dealInfo_price_sign"/>
            <com.sankuai.waimai.restaurant.shopcart.widget.text.StrikeTextView android:textSize="14sp" android:textColor="@color/wm_shopcart_text_hint" android:id="@+id/txt_dealInfo_original_price" android:focusable="false" android:focusableInTouchMode="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="3.75dp" android:maxLines="1" android:importantForAccessibility="no" app:layout_constraintBaseline_toBaselineOf="@+id/txt_dealInfo_price_sign" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/txt_dealInfo_price"/>
            <TextView android:textSize="11sp" android:textColor="#b2b2b2" android:id="@+id/txt_dealInfo_multi_tip" android:focusable="false" android:focusableInTouchMode="false" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="3.75dp" android:text="最终价格以提单为准" android:maxLines="1" android:importantForAccessibility="no" app:layout_constraintBaseline_toBaselineOf="@+id/txt_dealInfo_original_price" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/txt_dealInfo_original_price"/>
            <LinearLayout android:orientation="horizontal" android:id="@+id/ll_discount_detail" android:paddingLeft="4dp" android:paddingTop="4dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintStart_toEndOf="@+id/txt_dealInfo_multi_tip">
