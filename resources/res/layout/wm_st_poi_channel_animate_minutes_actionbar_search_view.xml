        </LinearLayout>
        <ImageView android:id="@+id/minutes_search_shop_cart_img" android:background="@drawable/wm_sc_search_shopcart_dark" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginLeft="13dp"/>
        <ImageView android:id="@+id/minutes_search_more_information_img" android:background="@drawable/wm_sc_search_more_information_dark" android:visibility="gone" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginLeft="10dp"/>
    </LinearLayout>
    <FrameLayout android:id="@+id/new_with_flowers_empty_layout" android:background="@color/transparent" android:visibility="gone" android:layout_width="match_parent" android:layout_height="5dp" android:layout_marginTop="10dp"/>
</FrameLayout>
