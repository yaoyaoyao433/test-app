            <ImageView android:id="@+id/minutes_iv_header_search_icon_left" android:background="@drawable/wm_st_actionbar_search" android:visibility="gone" android:layout_width="16dp" android:layout_height="16dp"/>
            <LinearLayout android:layout_gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginLeft="0dp" android:layout_marginRight="10dp" android:layout_weight="1">
                <TextView android:textSize="14sp" android:textColor="@color/wm_sg_color_999999" android:gravity="center" android:layout_gravity="center" android:id="@+id/minutes_tv_header_search_view" android:background="@android:color/transparent" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/wm_sc_common_search_shop_or_good_hint" android:singleLine="true" android:layout_centerVertical="true"/>
                <LinearLayout android:gravity="center" android:layout_gravity="center" android:id="@+id/minutes_buy_layout" android:background="@drawable/wm_st_search_minutes_bg" android:visibility="gone" android:layout_width="0dp" android:layout_height="0dp" android:layout_marginLeft="0dp" android:layout_marginTop="0.5dp" android:layout_centerVertical="true">
                    <TextView android:textSize="10sp" android:textColor="#61c454" android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="分钟购"/>
                </LinearLayout>
                <ViewFlipper android:id="@+id/minutes_vf_search_carousel_text" android:background="@android:color/transparent" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
            </LinearLayout>
            <ImageView android:id="@+id/minutes_ocr_camera" android:visibility="gone" android:layout_width="20dp" android:layout_height="20dp" android:layout_marginRight="10dp" android:src="@drawable/wm_sc_ocr_camera"/>
