<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:paddingLeft="16dp" android:paddingRight="16dp" android:paddingBottom="10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <View android:background="?attr/rooGrayLight" android:layout_width="match_parent" android:layout_height="0.5dp"/>
    <LinearLayout android:gravity="center_horizontal" android:orientation="horizontal" android:id="@+id/customer_service_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="13dp" android:layout_marginBottom="15dp">
        <TextView android:textSize="16sp" android:textColor="?attr/rooBrandPrimaryDark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/wm_order_detail_customer_service_tel"/>
        <TextView android:textSize="16sp" android:textColor="?attr/rooBrandPrimaryDark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="4dp" android:text="@string/wm_order_detail_customer_service_phone_number"/>
    </LinearLayout>
    <com.meituan.roodesign.widgets.button.RooButton android:id="@+id/button_cancel" android:paddingLeft="8dp" android:paddingRight="8dp" android:layout_width="match_parent" android:layout_height="40dp" android:text="@string/roo_bottomsheet_cancel" style="@style/Widget.RooDesign.Button.WeakButton"/>
</LinearLayout>
