<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="wrap_content">
    <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:id="@+id/normal_cell_view" android:layout_width="match_parent" android:layout_height="wrap_content">
            <FrameLayout android:layout_gravity="center_vertical" android:id="@+id/arrow_wrapper" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:id="@+id/arrow" android:layout_width="7dp" android:layout_height="wrap_content" android:src="@drawable/shieldc_arrow"/>
            </FrameLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/action_container" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </FrameLayout>
</merge>
