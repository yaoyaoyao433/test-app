<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:gravity="center_vertical" android:orientation="vertical" android:id="@+id/layout_actionbar_content" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/rl_action_home_content" android:layout_width="match_parent" android:layout_height="wrap_content">
        <ImageView android:id="@+id/iv_back" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@string/wm_sg_back" style="@style/WmSgCommonLeftBackArrowStyle"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/address_layout" android:paddingRight="@dimen/wm_sc_common_dimen_9" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_toLeftOf="@+id/rl_right_area" android:layout_toRightOf="@+id/iv_back" android:layout_centerVertical="true">
            <ImageView android:layout_gravity="bottom" android:id="@+id/main_location_icon_title" android:layout_width="wrap_content" android:layout_height="24dp" android:layout_marginBottom="-2dp"/>
            <View android:layout_gravity="bottom" android:id="@+id/location_line" android:background="#c9c5c2" android:layout_width="1px" android:layout_height="@dimen/wm_sc_common_dimen_12" android:layout_marginLeft="8dp" android:layout_marginRight="8dp" android:layout_marginBottom="2dp"/>
            <Space android:id="@+id/v_space_location" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1"/>
            <TextView android:textSize="14dp" android:textColor="@color/wm_st_common_text_title" android:layout_gravity="bottom" android:id="@+id/txt_kong_location" android:paddingTop="5dp" android:text="@string/wm_sc_progressbar_locating" android:maxEms="10" android:includeFontPadding="false" style="@style/WmStStoreText"/>
            <ImageView android:layout_gravity="bottom" android:id="@+id/iv_arrow_right" android:layout_width="12dp" android:layout_height="12dp" android:layout_marginBottom="2dp"/>
        </LinearLayout>
        <ImageView android:id="@+id/iv_main_sugoo_text" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="@dimen/wm_sc_common_dimen_20" android:adjustViewBounds="true" android:maxHeight="@dimen/wm_sc_common_dimen_20" android:layout_toRightOf="@+id/iv_back" android:layout_centerVertical="true"/>
        <TextView android:textSize="20sp" android:textColor="#33312d" android:ellipsize="end" android:id="@+id/tv_main_sugoo_text" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxEms="6" android:singleLine="true" android:layout_toRightOf="@+id/iv_back" android:layout_centerVertical="true"/>
        <LinearLayout android:id="@+id/rl_right_area" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" android:layout_centerVertical="true">
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/iv_menu" android:visibility="invisible" android:layout_width="@dimen/wm_sc_common_dimen_24" android:layout_height="@dimen/wm_sc_common_dimen_24" android:layout_marginRight="12dp"/>
            <FrameLayout android:layout_gravity="center_vertical" android:id="@+id/fl_unread_message_container" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="10dp"/>
            <ImageView android:layout_gravity="center_vertical" android:id="@+id/iv_navigation_activity" android:visibility="gone" android:layout_width="@dimen/wm_sc_common_dimen_60" android:layout_height="@dimen/wm_sc_common_dimen_24" android:layout_marginRight="12dp" android:scaleType="fitCenter"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/nav_mach_content" android:paddingTop="1.5dp" android:layout="@layout/wm_sg_nav_mach_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_toRightOf="@+id/iv_back" android:layout_centerVertical="true"/>
    </RelativeLayout>
</LinearLayout>
